#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>
#include "list.h"


struct Node {
	char *value;
	struct Node *next;
};

void init(struct Node *head)
{
    head = NULL;
}

void push(struct Node *head,char *data) {
    struct Node *tmp = (struct Node*)malloc(sizeof(struct Node));
    if(tmp == NULL) {
        exit(0);
    }
    tmp->value = data;
    tmp->next = head;
    head = tmp;
}

void printlist(struct Node *head)
{
    struct Node *current;
    current = head;
    if(current!= NULL)
    {
        printf("Stack: ");
        do
        {
            printf("%s ",current->value);
            current = current->next;
        }
        while (current!= NULL);
        printf("\n");
    }
    else
    {
        printf("The Stack is empty\n");
    }

}
