#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>



struct Node {
	char *value;
	struct Node *next;
};

void init(struct Node *head)
{
	head->next = NULL;
	head->value = NULL;
}

void push(struct Node *head,char *data) {
	struct Node *tmp = (struct Node*)malloc(sizeof(struct Node));
	if(head->value == NULL) {
		tmp->value = data;
		tmp->next = NULL;
		*head = *tmp;
		printf("NULL ADD");
	}
	else {
		tmp->value = data;
		tmp->next = head;
		*head = *tmp;
		printf("NOT NULL ADD");
	}
}

void printlist(struct Node *head) {
	    //Fill in the code here
	    struct Node *temp = head;
	    if (temp == NULL)
	        printf(" {}");
	    //while (temp != NULL) {
	        printf(" %s", temp->value);
	        temp = temp->next;
	        printf(" %s", temp->value);
	       	        temp = temp->next;
	   // }
	}
